{
  "name": "nust",
  "version": "0.0.2",
  "description": "NestJS + Vue 3 + Vite",
  "author": "butnothero",
  "private": true,
  "type": "module",
  "module": "dist/main.js",
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "--- build ---": "",
    "build": "nest build",
    "build:csr": "ts-node --esm scripts/build-csr",
    "build:ssr": "ts-node --esm scripts/build-ssr",
    "build:pages": "ts-node --esm scripts/build-pages",
    "build:html": "ts-node --esm scripts/build-html",
    "postbuild": "npm run build:html && npm run build:pages && npm run build:csr && npm run build:ssr && npm run clean:html",
    "--- dev ---": "",
    "start": "npm run clean:generated && npm run clean:dist && cross-env NODE_ENV=development nest start",
    "watch": "npm run clean:generated && npm run clean:dist && cross-env NODE_ENV=development nest start --watch",
    "debug": "npm run clean:generated && npm run clean:dist && cross-env NODE_ENV=development nest start --debug --watch",
    "--- prod ---": "",
    "server": "cross-env NODE_ENV=production PORT=3333 node dist/main",
    "prod": "npm run clean:dist && npm run build && npm run server",
    "deploy": "npm run clean:dist && npm run build",
    "--- code style ---": "",
    "fix": "npm run fix:eslint && npm run fix:prettier && npm run fix:stylelint",
    "fix:eslint": "eslint **/*.{js,ts,vue} --fix",
    "fix:prettier": "prettier --write **/*.{vue,js,ts,css,sass,scss}",
    "fix:stylelint": "stylelint --fix **/*.{vue,css,sass,scss} --ignore-path",
    "windicss:analysis": "windicss-analysis",
    "--- test (SOON) ---": "",
    "test": "",
    "test:watch": "",
    "test:cov": "",
    "test:debug": "",
    "test:e2e": "",
    "--- clean ---": "",
    "clean:dist": "rimraf dist",
    "clean:generated": "rimraf client/auto-imports.d.ts && rimraf client/components.d.ts",
    "clean:html": "rimraf ./index.html"
  },
  "dependencies": {
    "@egjs/hammerjs": "^2.0.17",
    "@nestjs/common": "^9.0.4",
    "@nestjs/core": "^9.0.4",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/platform-express": "^9.0.4",
    "@nuxt/devalue": "2.0.0",
    "@sentry/tracing": "7.7.0",
    "@sentry/vue": "7.7.0",
    "@types/lodash": "^4.14.182",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vueuse/core": "^8.3.1",
    "@vueuse/head": "^0.7.6",
    "@vueuse/integrations": "^8.3.1",
    "axios": "^0.27.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "fast-glob": "^3.2.11",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.9",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "swiper": "^8.3.1",
    "universal-cookie": "^4.0.4",
    "vite": "^3.0.2",
    "vue": "^3.2.26",
    "vue-demi": "^0.13.5",
    "vue-i18n": "9.2.0-beta.35",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.4",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@prettier/plugin-pug": "^2.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.6",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@vue/eslint-config-typescript": "^11.0.0",
    "critters": "^0.0.16",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.2.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.5.1",
    "pug": "^3.0.2",
    "sass": "^1.51.0",
    "source-map-support": "^0.5.21",
    "stylelint": "^14.8.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.2.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.9.3",
    "unplugin-vue-components": "^0.21.1",
    "vite-plugin-inspect": "^0.6.0",
    "vite-plugin-pages": "^0.25.0",
    "vite-plugin-pwa": "^0.12.0",
    "vite-plugin-vue-layouts": "^0.6.0",
    "vite-plugin-windicss": "^1.8.4",
    "windicss-analysis": "^0.3.5",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "commit-msg": "commitlint -e -V",
    "pre-commit": "lint-staged",
    "pre-push": "lint-staged"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx}": "eslint --fix",
    "*.{vue,js,ts,jsx,tsx,css,sass,scss}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  }
}
